{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "questions-get": {
        "ClusterId": "questions-cluster",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "/api/questions",
          "Methods": ["GET"]
        }
      },
      "questions-write": {
        "ClusterId": "questions-cluster",
        "AuthorizationPolicy": "authenticated",
        "Match": {
          "Path": "/api/questions",
          "Methods": ["POST", "PUT", "DELETE", "PATCH"]
        }
      },
      "questions-catchall-get": {
        "ClusterId": "questions-cluster",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "/api/questions/{**catch-all}",
          "Methods": ["GET"]
        }
      },
      "questions-catchall-write": {
        "ClusterId": "questions-cluster",
        "AuthorizationPolicy": "authenticated",
        "Match": {
          "Path": "/api/questions/{**catch-all}",
          "Methods": ["POST", "PUT", "DELETE", "PATCH"]
        }
      },
      "answers-get": {
        "ClusterId": "answers-cluster",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "/api/answers",
          "Methods": ["GET"]
        }
      },
      "answers-write": {
        "ClusterId": "answers-cluster",
        "AuthorizationPolicy": "authenticated",
        "Match": {
          "Path": "/api/answers",
          "Methods": ["POST", "PUT", "DELETE", "PATCH"]
        }
      },
      "answers-catchall-get": {
        "ClusterId": "answers-cluster",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "/api/answers/{**catch-all}",
          "Methods": ["GET"]
        }
      },
      "answers-catchall-write": {
        "ClusterId": "answers-cluster",
        "AuthorizationPolicy": "authenticated",
        "Match": {
          "Path": "/api/answers/{**catch-all}",
          "Methods": ["POST", "PUT", "DELETE", "PATCH"]
        }
      },
      "votes-get": {
        "ClusterId": "votes-cluster",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "/api/votes",
          "Methods": ["GET"]
        }
      },
      "votes-write": {
        "ClusterId": "votes-cluster",
        "AuthorizationPolicy": "authenticated",
        "Match": {
          "Path": "/api/votes",
          "Methods": ["POST", "PUT", "DELETE", "PATCH"]
        }
      },
      "votes-catchall-get": {
        "ClusterId": "votes-cluster",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "/api/votes/{**catch-all}",
          "Methods": ["GET"]
        }
      },
      "votes-catchall-write": {
        "ClusterId": "votes-cluster",
        "AuthorizationPolicy": "authenticated",
        "Match": {
          "Path": "/api/votes/{**catch-all}",
          "Methods": ["POST", "PUT", "DELETE", "PATCH"]
        }
      },
      "users-exists": {
        "ClusterId": "users-cluster",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "/api/users/{id}/exists"
        }
      },
      "users-protected": {
        "ClusterId": "users-cluster",
        "AuthorizationPolicy": "authenticated",
        "Match": {
          "Path": "/api/users/{**catch-all}"
        }
      },
      "auth": {
        "ClusterId": "authentication-cluster",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "/api/auth/{**catch-all}"
        }
      },
      "authentication": {
        "ClusterId": "authentication-cluster",
        "AuthorizationPolicy": "anonymous",
        "Match": {
          "Path": "/api/authentication/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "questions-cluster": {
        "Destinations": {
          "d1": {
            "Address": "http://question-service/"
          }
        }
      },
      "users-cluster": {
        "Destinations": {
          "d1": {
            "Address": "http://user-service/"
          }
        }
      },
      "votes-cluster": {
        "Destinations": {
          "d1": {
            "Address": "http://voting-service/"
          }
        }
      },
      "answers-cluster": {
        "Destinations": {
          "d1": {
            "Address": "http://answer-service/"
          }
        }
      },
      "authentication-cluster": {
        "Destinations": {
          "d1": {
            "Address": "http://identityserver-service/"
          }
        }
      }
    }
  }
}